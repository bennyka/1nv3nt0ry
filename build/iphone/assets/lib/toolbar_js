function createToolbar(t){var o=t.title||Ti.App.name,i=Ti.UI.createView({top:0,left:0,right:0,height:50,width:"100%",backgroundColor:{type:"linear",colors:["#80ffffff","#60ffffff","#00ffffff"],startPoint:{x:0,y:0},endPoint:{x:0,y:"100%"},backFillStart:!1}}),o=Ti.UI.createLabel({text:o,shadowColor:"#000000",shadowOffset:{x:0,y:-1},color:"#ffffff",font:{fontFamily:"Helvetica Neue",fontWeight:"bold",fontSize:"18dp"},textAlign:"center",height:50});if(i.add(o),t.leftNavButton){var e=Ti.UI.createView({left:5,center:{y:"50%"},height:Ti.UI.SIZE,width:Ti.UI.SIZE,layout:"horizontal"});i.add(e);for(var a in t.leftNavButton)e.add(t.leftNavButton[a])}if(t.rightNavButton){var r=Ti.UI.createView({right:5,center:{y:"50%"},height:Ti.UI.SIZE,width:Ti.UI.SIZE,layout:"horizontal"});i.add(r);for(var a in t.rightNavButton)r.add(t.rightNavButton[a])}var n=Ti.UI.createView({width:"100%",bottom:0,height:1,backgroundColor:"#ffffff"});return i.add(n),i}Ti.include("styles.js");