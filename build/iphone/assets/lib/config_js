var osname=Ti.Platform.osname,version=Ti.Platform.version,height=Ti.Platform.displayCaps.platformHeight,width=Ti.Platform.displayCaps.platformWidth,config={isTablet:"ipad"===osname||"android"===osname&&(width>899||height>899)};windowPath=config.isTablet?"android"===osname?"ui/tablet/android/":"ui/tablet/ios/":"android"===osname?"ui/handheld/android/":"ui/handheld/ios/",config.windowPath=windowPath,Ti.App.config=config;