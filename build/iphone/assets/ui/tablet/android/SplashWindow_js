function SplashWindow(){function e(){0!=i.size.height&&(winHeight=i.size.height,winWidth=i.size.width,Ti.App.Properties.setList("winDimensions",[winWidth,winHeight]),Ti.App.fireEvent("startApp"),i.removeEventListener("postlayout",e))}var i=Ti.UI.createWindow({backgroundColor:"transparent",navBarHidden:!0}),t=require("ui/common/SplashView"),o=new t({shelfWin:i});return i.add(o),o.addEventListener("shouldCloseView",function(){i.close()}),i.addEventListener("postlayout",e),i}module.exports=SplashWindow;