function SplashWindow(){function e(){0!=t.size.height&&(winHeight=t.size.height,winWidth=t.size.width,Ti.App.Properties.setList("winDimensions",[winWidth,winHeight]),Ti.App.fireEvent("startApp"),t.removeEventListener("postlayout",e))}var t=Ti.UI.createWindow({backgroundColor:"transparent",navBarHidden:!0,orientationModes:[Ti.UI.PORTRAIT,Ti.UI.UPSIDE_PORTRAIT]}),i=require("ui/common/StartView"),o=new i({shelfWin:t});return t.add(o),o.addEventListener("shouldCloseView",function(){t.close()}),t.addEventListener("postlayout",e),t}module.exports=SplashWindow;