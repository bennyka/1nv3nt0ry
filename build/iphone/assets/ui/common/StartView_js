function StartView(){var e=Ti.UI.createView({width:Ti.App.Properties.getList("winDimensions")[0],height:Ti.App.Properties.getList("winDimensions")[1],backgroundColor:backgroundStyle.backgroundColor}),t=createToolbar({title:Ti.App.name});e.add(t);var i=Ti.UI.createLabel({text:L("inventory"),top:e.height/3,width:128,height:30,left:10,font:{fontFamily:"Helvetica Neue",fontWeight:"bold",fontSize:"15dp"},textAlign:"center",color:"#ffffff"});e.add(i);var o=createButton({backgroundImage:"/images/inventory.png",height:128,width:128,left:10,top:i.top+i.height});e.add(o);var a=Ti.UI.createLabel({text:L("createObject"),top:e.height/3-25,width:128,height:50,right:10,font:{fontFamily:"Helvetica Neue",fontWeight:"bold",fontSize:"15dp"},textAlign:"center",color:"#ffffff"});e.add(a);var n=createButton({backgroundImage:"/images/object.png",height:128,width:128,right:10,top:a.top+a.height});e.add(n),n.addEventListener("click",function(){var e=require(Ti.App.config.windowPath+"ObjectWindow"),t=new e;t.open()});var r=createButton({title:L("imprint"),left:0,right:0,bottom:20,font:{fontFamily:"Helvetica Neue",fontWeight:"bold",fontSize:"15dp"}});return e.add(r),e}Ti.include("/lib/toolbar.js"),Ti.include("/lib/createButton.js"),Ti.include("/lib/styles.js"),module.exports=StartView;